sub getprdata {
&vars;

if(($FORM{'username'} eq "")||($FORM{'password'} eq "")){&missingUorP;}
unless(-e "$users/$FORM{'username'}.txt"){&problemencountered;}

open (FILE, "$users/$FORM{'username'}.txt") || &oops ($!, "Could not open $users/$username.txt");
flock (FILE, 1) or die "Cannot lock file\n";
while (<FILE>) {
chop;
	@datafile=split(/\n/);
	foreach $line (@datafile) {
	&edprofchku($line);
	}
	}
	close(FILE);
	

}

sub edprofchku($line){

local ($line) = @_;
	local ($loginname,$password,$emailaddr);
		  ($loginname,$password,$emailaddr)= split(/\|/, $line);

if(($FORM{'username'} eq "$username")&&($FORM{'password'} eq "$password")){&getprofdata;}
else {&problemencountered;}
}


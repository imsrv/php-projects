function formatStatusString($num, $folder, $type){
	$str="";
	if ($type=="delete"){
		$str = "Usunito $num wiadomo".($num>1?"ci":"");
	}else if ($type == "move"){
		$str = "Przeniesiono $num wiadomo".($num>1?"ci":"")." do $folder";
	}
	
	return $str;
}


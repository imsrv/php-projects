<!-- Needed JavaScript Code -->
<script type="text/javascript">
    var cleared = 0;

    function clearField(field)
    {
        if (cleared != 1) {
            field.value = ""
            cleared = 1
        } else {
            cleared = 0
        }
    }
</script>
{plg_headercode}

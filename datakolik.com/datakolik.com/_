 function setCookie(langcode){ var langcode=langcode;
if(langcode==null)langcode=document.AnonLangPrefSelectorForm.AnonLangPref.value;
 var today = new Date();
 var expDate = new Date(today.getTime() + 700 * 24 * 60 * 60 * 1000);  
var cookieExpires = expDate.toGMTString(); 
document.cookie='LANGPREF='+escape(langcode)+';expires='+cookieExpires + ';path=/;'
newwindow=window.open('/anacreon/servlet/MyChangeLangPrefServlet?langcode='+langcode,'','left=100,top=100,width=1,height=1,resizable=no,scrollbars=no');
 newwindow.blur();
newwindow.setTimeout('window.close()',500);
window.location.reload();
return false;
}</script>
